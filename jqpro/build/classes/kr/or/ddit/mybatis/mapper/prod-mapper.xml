<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	
	
<mapper namespace="prod">


<select id="getProdLgu" parameterType="String" resultType="prodVO">
select prod_id, prod_name from prod where prod_lgu = #{prod_lgu}
</select>

<select id="getProdId" parameterType="String" resultType="prodVO">
select * from prod where prod_id = #{prod_id}
</select>




</mapper>


